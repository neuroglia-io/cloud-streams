<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>$(MSBuildProjectName.Replace(" ", "_").Replace(".Http", ""))</RootNamespace>
	<GenerateDocumentationFile>True</GenerateDocumentationFile>
	<OutputType>Library</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Commands\**" />
    <Content Remove="Commands\**" />
    <EmbeddedResource Remove="Commands\**" />
    <None Remove="Commands\**" />
  </ItemGroup>
	
  <ItemGroup>
    <Folder Include="Configuration\" />
  </ItemGroup>
	
  <ItemGroup>
    <PackageReference Include="System.ServiceModel.Primitives" Version="4.10.0" />
  </ItemGroup>
	
  <ItemGroup>
    <ProjectReference Include="..\application\CloudStreams.Gateway.Application.csproj" />
    <ProjectReference Include="..\client\CloudStreams.Gateway.Api.Client.csproj" />
  </ItemGroup>

</Project>
